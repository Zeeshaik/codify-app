{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/pages/wordle/App.js", "src/pages/wordle/reportWebVitals.js", "src/pages/wordle/Words.js", "src/pages/wordle/index.js", "src/components/WordleCom/GameOver/GameOver.js", "src/components/WordleCom/Key/Key.js", "src/components/WordleCom/Keyboard/Keyboard.js", "src/components/WordleCom/Letter/Letter.js"],
  "exclude": ["node_modules"]
}
